#!/home/frafra/nina-data/ckan/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'ckan==2.9.0a0','console_scripts','ckan'
__requires__ = 'ckan==2.9.0a0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ckan==2.9.0a0', 'console_scripts', 'ckan')()
    )
